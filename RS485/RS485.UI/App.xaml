<Application x:Class="RS485.UI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:converter="clr-namespace:RS485.Common.Converters;assembly=RS485.Common"
             xmlns:converters="clr-namespace:RS485.UI.Helpers.Converters">
    <Application.Resources>
        <converters:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
        <converters:EnumToBooleanConverter x:Key="EnumToBooleanConverter" />
        <converters:InverseBooleanConverter x:Key="InverseBooleanConverter" />
        <converters:NullToBooleanConverter x:Key="NullToBooleanConverter" />
        <converters:NullToVisibilityConverter x:Key="NullToVisibilityConverter" />
        <converter:EnumToVisibilityConverter x:Key="EnumToVisibilityConverter" />
    </Application.Resources>
</Application>